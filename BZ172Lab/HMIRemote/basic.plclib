		<function name="ByteToWord" version="1.0.0" creationDate="0" lastModifiedDate="0">
			<descr>Compose a word from 2 bytes</descr>
			<returnValue>UINT</returnValue>
			<vars>
				<inputVars>
					<var name="byte0" type="BYTE">
						<descr>Low byte</descr>
					</var>
					<var name="byte1" type="BYTE">
						<descr>High byte</descr>
					</var>
				</inputVars>
			</vars>
			<iecDeclaration active="FALSE"/>
			<sourceCode type="IL">
				<![CDATA[
	(*---------- WORD in memoria IEEE --------*)
	(*										  *)
	(*	WORD -> | H byte1 | L byte0 |		  *)
	(*										  *)
	(*----------------------------------------*)

	(*	Low byte *)

	LD		byte0
	TO_UINT
	ST		ByteToWord

	(*	High byte *)

	LD		byte1
	TO_UINT
	SHL		8
	ADD		ByteToWord
	ST		ByteToWord

]]>
			</sourceCode>
		</function>
		<function name="BitToByte" version="1.0.0" creationDate="0" lastModifiedDate="0">
			<descr>Compose a byte from 8 bits</descr>
