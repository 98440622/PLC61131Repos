		<functionBlock name="Pulse" version="1.0.0" creationDate="0" lastModifiedDate="0">
			<descr>Pulse generator</descr>
			<vars>
				<inputVars>
					<var name="enable" type="BOOL">
						<descr>Enable Input</descr>
					</var>
					<var name="period" type="UINT">
						<descr>Period [number of function block call]</descr>
					</var>
				</inputVars>
				<outputVars>
					<var name="out" type="BOOL">
						<descr>Pulse Signal</descr>
					</var>
				</outputVars>
				<localVars>
					<var name="cnt" type="UINT"/>
				</localVars>
			</vars>
			<iecDeclaration active="FALSE"/>
			<interfaces/>
			<methods/>
			<sourceCode type="IL">
				<![CDATA[

		LD		enable
		JMPC	l0

		LD		0
		ST		cnt
		LD		FALSE
		ST		out
		RET

l0:

		LD		cnt
		ADD		1
		ST		cnt

		LD		cnt
		GE		period
		ST		out
		RETCN

l1:

		LD		0
		ST		cnt

]]>
			</sourceCode>
		</functionBlock>
		<functionBlock name="WordToByte" version="1.0.0" creationDate="0" lastModifiedDate="0">
			<descr>Split a word into two bytes</descr>
			<vars>
				<inputVars>
					<var name="w" type="UINT">
						<descr>Word Input</descr>
					</var>
				</inputVars>
				<outputVars>
					<var name="byte0" type="USINT">
						<descr>Low byte</descr>
					</var>
					<var name="byte1" type="USINT">
						<descr>High byte</descr>
					</var>
				</outputVars>
			</vars>
			<iecDeclaration active="FALSE"/>
			<interfaces/>
			<methods/>
			<sourceCode type="IL">
				<![CDATA[
	(*---------- WORD in memoria IEEE --------*)
	(*										  *)
	(*	WORD -> | H byte1 | L byte0 |		  *)
	(*										  *)
	(*----------------------------------------*)

	(*	Low byte *)

	LD		w
	TO_USINT
	ST		byte0

	(*	High byte *)

	LD		w
	SHR		8
	TO_USINT
	ST		byte1

]]>
			</sourceCode>
		</functionBlock>
